{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "ext-soap": "*",
        "ariaieboy/filament-currency": "^1.10",
        "asmit/resized-column": "^1.1",
        "awcodes/filament-sticky-header": "^2.0",
        "awcodes/filament-table-repeater": "^3.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "barryvdh/laravel-snappy": "^1.0",
        "codewithdennis/filament-theme-inspector": "^0.0.7",
        "dompdf/dompdf": "^3.1",
        "dvarilek/filament-table-select": "^2.0",
        "eclipxe/cfdiutils": "^2.29",
        "eightynine/filament-excel-import": "^3.1",
        "eightynine/filament-reports": "^3.0",
        "filament/filament": "^3.2",
        "jimmyjs/laravel-report-generator": "^2.2",
        "joaopaulolndev/filament-pdf-viewer": "^1.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "luecano/numero-a-letras": "^4.0",
        "maatwebsite/excel": "^3.1",
        "nahidulhasan/html2pdf": "^1.4",
        "owenvoke/blade-fontawesome": "^2.6",
        "pelmered/filament-money-field": "^1.4",
        "phpcfdi/cfdi-expresiones": "^3.2",
        "phpcfdi/cfditopdf": "^0.4.2",
        "phpcfdi/sat-ws-descarga-masiva": "^0.5.4",
        "phpmailer/phpmailer": "^6.10",
        "phpoffice/phpspreadsheet": "^1.29",
        "solution-forest/filament-simplelightbox": "^0.0.10",
        "torgodly/html2media": "^1.1",
        "ext-calendar": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
